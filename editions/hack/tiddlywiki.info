{
  "description": "Theme development edition",
  "plugins": [
    "tiddlywiki/github-fork-ribbon",
    "tiddlywiki/filesystem",
    "tiddlywiki/tiddlyweb",
    "ustuehler/caption",
    "ustuehler/core",
    "ustuehler/github",
    "ustuehler/mdc",
    "ustuehler/mdc-web",
    "ustuehler/mdc-web-cdn",
    "ustuehler/muuri"
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite",
    "ustuehler/material"
  ],
  "includeWikis": {
    "path": "../theme"
  },
  "build": {
    "index": [
      "--output",".",
      "--rendertiddler","$:/core/save/all","hack.html","text/plain"
    ]
  },
  "config": {
    "retain-original-tiddler-path": true
  }
}
